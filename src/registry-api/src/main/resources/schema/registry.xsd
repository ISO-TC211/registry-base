<xs:schema xmlns:gdireg="http://registry.geoinfoffm.de/" xmlns:grg="http://www.isotc211.org/2005/grg" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:gts="http://www.isotc211.org/2005/gts" xmlns:whttp="http://www.w3.org/ns/wsdl/http" xmlns:wsdl="http://www.w3.org/ns/wsdl" xmlns:wsoap="http://www.w3.org/ns/wsdl/soap" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://registry.geoinfoffm.de/" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.xmlsoap.org/soap/encoding/" schemaLocation="http://schemas.xmlsoap.org/soap/encoding/"/>
	<xs:import namespace="http://www.isotc211.org/2005/grg" schemaLocation="iso/19135/grg.xsd"/>
	<xs:import namespace="http://www.isotc211.org/2005/gco" schemaLocation="iso/19139/gco/gco.xsd"/>
	<xs:import namespace="http://www.isotc211.org/2005/gmd" schemaLocation="iso/19139/gmd/gmd.xsd"/>
	<!--<xs:import namespace="http://www.isotc211.org/2005/grg" schemaLocation="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19135-2_Schemas/grg.xsd"/>
	<xs:import namespace="http://www.isotc211.org/2005/gco" schemaLocation="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/gco/gco.xsd"/>-->
	<!-- =========================================================== -->
	<!-- Role_Type -->
	<!-- ........................................................................ -->
	<xs:complexType name="Role_Type">
		<xs:sequence>
			<xs:element name="roleName" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ........................................................................ -->
	<xs:element name="Role" type="gdireg:Role_Type"/>
	<!-- ........................................................................ -->
	<xs:complexType name="Role_PropertyType">
		<xs:sequence>
			<xs:element ref="gdireg:Role" minOccurs="0"/>
		</xs:sequence>
		<xs:attributeGroup ref="gco:ObjectReference"/>
		<xs:attribute ref="gco:nilReason"/>
	</xs:complexType>
	<!-- =========================================================== -->
	<!-- Authorization_Type -->
	<!-- ........................................................................ -->
	<xs:complexType name="Authorization_Type">
		<xs:sequence>
			<xs:element name="actor" type="gdireg:Actor_PropertyType"/>
			<xs:element name="role" type="gdireg:Role_PropertyType"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ........................................................................ -->
	<xs:element name="Authorization" type="gdireg:Authorization_Type"/>
	<!-- ........................................................................ -->
	<xs:complexType name="Authorization_PropertyType">
		<xs:sequence>
			<xs:element ref="gdireg:Authorization" minOccurs="0"/>
		</xs:sequence>
		<xs:attributeGroup ref="gco:ObjectReference"/>
		<xs:attribute ref="gco:nilReason"/>
	</xs:complexType>
	<!-- =========================================================== -->
	<!-- Delegation_Type -->
	<!-- ........................................................................ -->
	<xs:complexType name="Delegation_Type">
		<xs:complexContent>
			<xs:extension base="gdireg:Authorization_Type">
				<xs:sequence>
					<xs:element name="delegatingOrganization" type="gdireg:Organization_PropertyType"/>
					<xs:element name="isApproved" type="xs:boolean"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ........................................................................ -->
	<xs:element name="Delegation" type="gdireg:Delegation_Type"/>
	<!-- ........................................................................ -->
	<xs:complexType name="Delegation_PropertyType">
		<xs:sequence>
			<xs:element ref="gdireg:Delegation" minOccurs="0"/>
		</xs:sequence>
		<xs:attributeGroup ref="gco:ObjectReference"/>
		<xs:attribute ref="gco:nilReason"/>
	</xs:complexType>
	<!-- =========================================================== -->
	<!-- Actor_Type -->
	<!-- ........................................................................ -->
	<xs:complexType name="AbstractActor_Type" abstract="true">
		<xs:sequence>
			<xs:element name="authorization" type="gdireg:Authorization_PropertyType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ........................................................................ -->
	<xs:element name="AbstractActor" type="gdireg:AbstractActor_Type" abstract="true"/>
	<!-- ........................................................................ -->
	<xs:complexType name="Actor_PropertyType">
		<xs:sequence>
			<xs:element ref="gdireg:AbstractActor" minOccurs="0"/>
		</xs:sequence>
		<xs:attributeGroup ref="gco:ObjectReference"/>
		<xs:attribute ref="gco:nilReason"/>
	</xs:complexType>
	<!-- =========================================================== -->
	<!-- Organization_Type -->
	<!-- ........................................................................ -->
	<xs:complexType name="Organization_Type">
		<xs:complexContent>
			<xs:extension base="gdireg:AbstractActor_Type">
				<xs:sequence>
					<xs:element name="shortName" type="gco:CharacterString_PropertyType"/>
					<xs:element name="name" type="gco:CharacterString_PropertyType"/>
					<xs:element name="submittingOrganization" type="grg:RE_SubmittingOrganization_PropertyType"/>
					<xs:element name="address" type="gmd:CI_Address_PropertyType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ........................................................................ -->
	<xs:element name="Organization" type="gdireg:Organization_Type"/>
	<!-- ........................................................................ -->
	<xs:complexType name="Organization_PropertyType">
		<xs:sequence>
			<xs:element ref="gdireg:Organization" minOccurs="0"/>
		</xs:sequence>
		<xs:attributeGroup ref="gco:ObjectReference"/>
		<xs:attribute ref="gco:nilReason"/>
	</xs:complexType>
</xs:schema>